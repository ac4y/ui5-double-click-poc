{
  "name": "ui5-double-click-poc",
  "version": "2.0.0",
  "description": "SAPUI5 POC for double-click editable input - hybrid runner",
  "scripts": {
    "start": "npx ui5 serve --port 8200 --open index.html",
    "start:cdn": "npx http-server -p 8200 -o index.html?env=cdn",
    "start:local": "npx ui5 serve --port 8200 --open index.html?env=local",
    "start:backend": "npx http-server -p 8200 -o index.html?env=backend",
    "start:hybrid": "npx ui5 serve --port 8200 --config ui5-backend.yaml --open index.html?env=hybrid"
  },
  "devDependencies": {
    "@ui5/cli": "^3.0.0",
    "ui5-middleware-simpleproxy": "^0.9.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
