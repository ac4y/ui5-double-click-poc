{
  "_version": "1.12.0",
  "sap.app": {
    "id": "ui5.doubleclick.poc",
    "type": "application",
    "title": "Double-Click POC",
    "description": "UI5 POC for double-click editable input",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ui5.doubleclick.poc.view.Main",
      "type": "XML",
      "id": "main"
    },
    "dependencies": {
      "minUI5Version": "1.105.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "ui5.doubleclick.poc.view",
        "controlId": "main",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "main",
          "target": "main"
        }
      ],
      "targets": {
        "main": {
          "viewName": "Main",
          "viewLevel": 1
        }
      }
    }
  }
}
